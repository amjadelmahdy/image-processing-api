{
  "name": "image_processing_api",
  "version": "1.0.0",
  "description": "Express.js image processing api",
  "main": "./build/index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "npm run build && nodemon build/index.js",
    "dev:start": "nodemon src/index.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --config .prettierrc src/**/*.ts --write",
    "prettier:fix": "npm run prettier && npm run lint:fix",
    "test": "db-migrate db:create ipapi_test && db-migrate --env test up && cross-env ENV=test ts-node node_modules/jasmine/bin/jasmine && db-migrate db:drop ipapi_test"
  },
  "author": "Amjad Elmahdy",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@types/express": "^4.17.21",
    "@types/jasmine": "^5.1.4",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.11",
    "@types/pg": "^8.11.6",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.2.0",
    "jasmine": "^5.1.0",
    "jasmine-spec-reporter": "^7.0.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "db-migrate": "^0.11.14",
    "db-migrate-pg": "^1.5.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.5",
    "sharp": "^0.33.4",
    "winston": "^3.13.0"
  }
}
